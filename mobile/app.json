        "foregroundImage": "./native/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "io.proviant.android"
    },
    "web": {
      "favicon": "./native/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b1332fff-79cc-4850-8357-7eb9f54a974a"
      }
    }
  }
