{
  "expo": {
    "name": "Proviant.io",
    "slug": "proviant-io",
    "scheme": "proviant-io",
    "owner": "proviant",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./native/assets/icon.png",
