{
  "expo": {
    "name": "Proviant",
    "slug": "proviant",
    "owner": "proviant",
    "version": "0.0.45",
    "orientation": "portrait",
    "icon": "./native/assets/icon.png",
    "splash": {
      "image": "./native/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.proviant.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./native/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "io.proviant.mobile"
    },
    "web": {
      "favicon": "./native/assets/favicon.png"
    }
  }
}
